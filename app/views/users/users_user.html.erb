<%
breadcrumbs = [
	{active: false, href: root_path, text: t('menu.initial_page')},
	{active: false, href: users_user_path, text: User.human_attribute_name(:common_users)},
	{active: true, href: '#', text: t('model.list')},
]
%>

<%= 
render 'common_pages/index', 
attributes: {
	page_title: User.human_attribute_name(:common_users), 
	url_new_path: new_user_path(profile_id: Profile::USER_ID), 
	policy_true: policy(User).new?, 
	objects: @users, 
	objects_path: users_user_path, 
	object_name: User.human_attribute_name(:common_user),
	breadcrumbs: breadcrumbs,
	objects_path_export: users_user_path(format: "csv", users_grid: (params[:users_grid].nil? ? "" : params[:users_grid].to_enum.to_h))
}
%>